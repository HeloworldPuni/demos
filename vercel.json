{
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "installCommand": "npm install --legacy-peer-deps",
    "devCommand": "npm run dev",
    "framework": "nextjs",
    "regions": [
        "iad1"
    ],
    "crons": [
        {
            "path": "/api/cron/daily-leaderboard",
            "schedule": "0 0 * * *"
        },
        {
            "path": "/api/cron/index-events",
            "schedule": "0 12 * * *"
        }
    ]
}